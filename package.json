{
    "name": "react-native-orientation-manager",
    "version": "1.0.2",
    "description": "A React Native module to retrieve interface/device orientation, listen to orientation changes, and lock screen to a specific orientation.",
    "main": "lib/commonjs/index",
    "module": "lib/module/index",
    "types": "lib/typescript/src/index.d.ts",
    "react-native": "src/index",
    "source": "src/index",
    "files": [
        "src",
        "lib",
        "android",
        "ios",
        "windows",
        "cpp",
        "*.podspec",
        "!ios/build",
        "!android/build",
        "!android/gradle",
        "!android/gradlew",
        "!android/gradlew.bat",
        "!android/local.properties",
        "!**/__tests__",
        "!**/__fixtures__",
        "!**/__mocks__",
        "!**/.*",
        "!windows/*AppPackages*",
        "!windows/*BundleArtifacts*",
        "!windows/[Tt]humbs.db",
        "!windows/*.DS_Store",
        "!windows/*.[Oo]bj",
        "!windows/*.user",
        "!windows/*.aps",
        "!windows/*.pch",
        "!windows/*.vspscc",
        "!windows/*.vssscc",
        "!windows/*_i.c",
        "!windows/*_p.c",
        "!windows/*.ncb",
        "!windows/*.suo",
        "!windows/*.tlb",
        "!windows/*.tlh",
        "!windows/*.bak",
        "!windows/*.[Cc]ache",
        "!windows/*.ilk",
        "!windows/*.log",
        "!windows/*.lib",
        "!windows/*.sbr",
        "!windows/*.sdf",
        "!windows/*.opensdf",
        "!windows/*.opendb",
        "!windows/*.unsuccessfulbuild",
        "!windows/ipch/",
        "!windows/[Oo]bj/",
        "!windows/[Bb]in",
        "!windows/[Dd]ebug*/",
        "!windows/[Rr]elease*/",
        "!windows/Ankh.NoLoad",
        "!windows/ipch/",
        "!windows/*.aps",
        "!windows/*.ncb",
        "!windows/*.opendb",
        "!windows/*.opensdf",
        "!windows/*.sdf",
        "!windows/*.cachefile",
        "!windows/*.VC.db",
        "!windows/*.VC.VC.opendb",
        "!windows/*.pidb",
        "!windows/*.userprefs",
        "!windows/_ReSharper*/",
        "!windows/*.resharper",
        "!windows/[Tt]est[Rr]esult*",
        "!windows/*.sass-cache",
        "!windows/[Bb]uild/",
        "!windows/.svn",
        "!windows/~$*",
        "!windows/*~",
        "!windows/packages/",
        "!windows/*.nupkg",
        "!windows/*ncrunch*",
        "!windows/*crunch*.local.xml",
        "!windows/*.dbmdl",
        "!windows/*.testsettings",
        "!windows/*.DotSettings",
        "!windows/.vs/",
        "!windows/*project.lock.json",
        "!windows/**/Generated Files/**"
    ],
    "scripts": {
        "test": "jest",
        "typecheck": "tsc --noEmit",
        "lint": "eslint \"**/*.{js,ts,tsx}\"",
        "clean": "del-cli android/build lib",
        "prepare": "bob build",
        "release": "release-it"
    },
    "keywords": [
        "react-native",
        "ios",
        "android",
        "windows"
    ],
    "repository": "https://github.com/KroosX4V/react-native-orientation-manager",
    "author": "KroosX4V <hasxt23@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/KroosX4V/react-native-orientation-manager/issues"
    },
    "homepage": "https://github.com/KroosX4V/react-native-orientation-manager#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^17.0.2",
        "@react-native/eslint-config": "^0.72.2",
        "@release-it/conventional-changelog": "^5.0.0",
        "@types/jest": "^28.1.2",
        "@types/react": "~17.0.21",
        "@types/react-native": "0.70.0",
        "commitlint": "^17.0.2",
        "del-cli": "^5.0.0",
        "eslint": "^8.4.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^28.1.1",
        "prettier": "^2.0.5",
        "react": "18.2.0",
        "react-native": "0.72.5",
        "react-native-builder-bob": "^0.20.0",
        "react-native-windows": "0.72.10",
        "release-it": "^15.0.0",
        "turbo": "^1.10.7",
        "typescript": "^5.0.2"
    },
    "resolutions": {
        "@types/react": "17.0.21"
    },
    "peerDependencies": {
        "@react-navigation/native": ">= 5.0.0",
        "react": "*",
        "react-native": "*"
    },
    "peerDependenciesMeta": {
        "@react-navigation/native": {
            "optional": true
        }
    },
    "packageManager": "yarn@3.6.1",
    "engines": {
        "node": ">= 16.0.0"
    },
    "jest": {
        "preset": "react-native",
        "modulePathIgnorePatterns": [
            "<rootDir>/lib/"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "release-it": {
        "git": {
            "commitMessage": "chore: release ${version}",
            "tagName": "v${version}"
        },
        "npm": {
            "publish": true
        },
        "github": {
            "release": true
        },
        "plugins": {
            "@release-it/conventional-changelog": {
                "preset": "angular"
            }
        }
    },
    "eslintConfig": {
        "root": true,
        "extends": [
            "@react-native",
            "prettier"
        ],
        "rules": {
            "prettier/prettier": [
                "error",
                {
                    "quoteProps": "consistent",
                    "singleQuote": false,
                    "tabWidth": 4,
                    "trailingComma": "es5",
                    "useTabs": false
                }
            ]
        }
    },
    "eslintIgnore": [
        "node_modules/",
        "lib/"
    ],
    "prettier": {
        "quoteProps": "consistent",
        "singleQuote": false,
        "tabWidth": 4,
        "trailingComma": "es5",
        "useTabs": false
    },
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "commonjs",
            "module",
            [
                "typescript",
                {
                    "project": "tsconfig.json"
                }
            ]
        ]
    }
}